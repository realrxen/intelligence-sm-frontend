.bg-transparent {
	position: relative;
	/* background-image:url(https://ossweb-img.qq.com/images/lol/web201310/skin/big91005.jpg); */
}
.bg-transparent::before {
	content: "";
	display: block;
	background: inherit;
	-webkit-filter: blur(10rpx);
	        filter: blur(10rpx);
	position: absolute;
	width: 100%;
	height: 100%;
	top: 10rpx;
	left: 10rpx;
	/* z-index: -1; */
	opacity: 0.1;
	-webkit-transform-origin: 0 0;
	        transform-origin: 0 0;
	border-radius: inherit;
	-webkit-transform: scale(1, 1);
	        transform: scale(1, 1);
}

